/* Travel Site Styles */

/* CSS Basic Document */
#wrapper {
	padding : 10px 10px 5px 5px;
	width : 1200px;
	height : 100%;
	margin-left : auto;
	margin-right : auto;
	text-align: center;
}

.floatright { 
	float: right; 
	margin: 0px 0px 5px 5px; 
	
}

#banner {
	border : 0;
	width : 1200px;
	height : 63px;
}


#nav {
	border : 0;
	width : 125px;
	height : 100%;
	margin : 10px 0;
	float : left;
}

#about {
	border-left : 1px solid #d7d8da;
	width : 1200px;
	float : left;
	margin : 10px 0 0 0px;
	padding : 0 0 0 0px;
}

#footer {
	width : 1200px;
	height: 150px
	padding : 20 20 20 20px;
	background-color: #666;
	padding-left: 20px;
	padding-right: 20px;
}


#content {
	opacity: 0.75;
	width : 1200px;
	height: 150px
	margin : 10px 0 0 0px;
	padding :20 20 20 20px;
	
}
#quote1 {
	opacity: 0.75;
	width : 1200px;
	margin : 10px 0 0 0px;
	padding : 20 20 20 20px;
	line-height: inherit;
}

#contentsecondary {
	opacity: 0.75;
	width : 1200px;
	margin : 10px 0 0 0px;
	padding :0 0 0 20px;
	line-height: inherit;
}

#aboutme {
	width : 1200px;
    border-left : 1px solid #d7d8da;
	width : 1200px;
	float : left;
	margin : 10px 0 0 0px;
	padding : 0 0 0 10px;
}

#content.special {
	border-left : 0 solid #d7d8da;
	width : 1200px;
	min-height : 420px;
	margin : 10px 0 10px 0px;
	padding : 0 0 0 10px;
}

/* CSS Background Image Document */
body {
	background-color : #fff;
	background-image : url(../images/690x1000bkg.jpg);
	background-repeat : no-repeat;
	background-position : center;
}




/* CSS Text Document */
body {
	font-family : Helvetica, Arial, Verdana, sans-serif;
	font-size : 24px;
	font-style : normal;
	line-height : 24px;
	font-weight : normal;
	font-variant : normal;
	text-transform : none;
	color : #FFFFFF;
	text-decoration : none;
	float: left;
}


h1 {
	color : #58595b;
	font-size : 45px;
	font-weight : normal;
}

h2 {
	color : #990000;
	font-size : 45px;
	font-weight : normal;
}

h3 {
	color : #58595b;
	font-size : 25px;
	font-weight : normal;
	text-align: left;
	border-top-width: thick;
}


th {
	text-align : left;
}




/* Navigation Styles */
#links li a {
	color : #000;
	text-decoration : none;
}


a:link, a:visited {
	color : #000;
}


#links li a:hover {
	color : #ff4e00;
	background-color : #f99f03;
}


p a:link, p a:visited {
	color : #6db7da;
}
