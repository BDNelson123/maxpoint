<script type="text/javascript">
		
		
		/*Global Variable
		nc_name: name of user
		nc_email_address: email address of user
		nc_company_name: company name of user
		
		all_blank_fields: flag to see if all fields are blank at the same time
		valid_name: flag for the name field
		valid_company_name: flag for the company name field
		valid_email: flag for the email field
		*/
		
		nc_name = ""
		nc_company_name = ""
		nc_email_address = ""
		
		all_blank_fields="true";
		valid_name="true";
		valid_company_name="true";
		valid_email="true";
		
		
		//---Dispatch Validator() when submit button is clicked---//
		$('#submit').click(function () {
			nc_name = $("#name").val();
			nc_company_name = $("#company").val();
			nc_email_address = $("#email").val();

			validator();
				
			if ((valid_name == "false") || (valid_email == "false") || (valid_company_name == "false")) {
				return false;
			}
			else {
				return true;
			}	
        });
		
	
		//---Function that validates fields
		function validator() {
			document.getElementById('name').style.backgroundColor='#e0e0e0';
			document.getElementById('email').style.backgroundColor='#e0e0e0';
			document.getElementById('company').style.backgroundColor='#e0e0e0';
			
			if (nc_name == "") {
				$(".hideError_name").addClass("showError_name");
				valid_name="false";
			} 
			else {
				valid_name="true";
			}
			
			var atpos=nc_email_address.indexOf("@");
			var dotpos=nc_email_address.lastIndexOf(".");
			if (atpos<1 || dotpos<atpos+2 || dotpos+2>=nc_email_address.length){
				$(".hideError_email_address").addClass("showError_email_address");
				valid_email="false";
			}
			else {
				valid_email="true";
			}
			
			if (nc_company_name == "") {
				$(".hideError_company_name").addClass("showError_company_name");
				valid_company_name="false";
			} 
			else {
				valid_company_name="true";
			}
			
		}
		
		
		//---Remove error message when the field is focused---//
		$('#name').focus(function () {
            $(".hideError_name").removeClass("showError_name");
        });
		
		$('#email').focus(function () {
            $(".hideError_email_address").removeClass("showError_email_address");
        });
		$('#company').focus(function () {
            $(".hideError_company_name").removeClass("showError_company_name");
        });
		
	
    </script> 

