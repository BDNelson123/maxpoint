/*
* Colors:
* Text grey/gray: #3e4545
* Link blue/green: #106470
* Red: #9c3022
*/

* {
	outline: 0;
}

body {
	background: #f7f7f7;
	color: #3e4545;
	font-size: 12px;
	font-family: "Arial", Tahoma, sans-serif;
}

strong {
	font-weight: bold;
}

a {
	color: #106470;
	text-decoration: none;
	font-weight: bold;
}

a:hover {
	text-decoration: underline;
}

p.last-child {
	margin-bottom: 0 !important;
}

small {
	font-size: 10px;
	font-style: italic;
}

sup {
	font-size: 8px;
}

ol li {
	margin: 20px;
	list-style-type: decimal;
}

div#wrapper {
	width: 950px;
	margin: 0 auto;
}

div.feature.insights h2 {
    color: #9C3022;
    font-size: 16px;
    margin: 15px 0 5px;
}

div#ribbon {
	position: absolute;
	right: 0;
	top: 0;
	width: 159px;
	height: 97px;
	background: url('assets/images/header-ribbon.png') no-repeat transparent;
	text-indent: -9999px;
}

div#ribbon a {
	position: absolute;
	width: 108px;
	height: 69px;
	right: 0;
	top: 0;
	text-indent: -9999px;
}

div#header {
	width: 950px;
	height: 100px;
	float: left;
}

div#header h1 {
	width: 160px;
