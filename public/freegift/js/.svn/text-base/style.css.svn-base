}

div#sidebar {
	width: 230px;
	float: left;
	margin: 25px 25px 0 0;
}

div#sidebar div.widget {
	behavior: url('/PIE.htc');
	min-width: 228px;
	margin: 0 0 10px 0;
	float: left;
}

