	    linear,
	    left bottom,
	    left top,
	    color-stop(0.3, #7E2218),
	    color-stop(0.65, #9C3022)
	);
	background: -moz-linear-gradient(
	    center bottom,
	    #7E2218 30%,
	    #9C3022 65%
	);
