div#content div.feature.moms-tabs div.pane table td.result {
	margin-left: 25px;
	color: #9C3022;
}
div#content div.feature.moms-tabs div.pane div.content {
	float: left;
	width: 288px;
	margin-right: 40px;
}
div#content div.feature.moms-tabs div.pane div.map {
	float: left;
}

div#content div.feature.moms-foot {
	min-height: 0px;
	margin: 0 0 20px 0;
}

div#content div.feature.moms-foot h3 {
	font-size: 16px;
	line-height: 18px;
	margin: 0 0 15px 0;
}

div#content div.moms-tabs-nav ul.tabs li {
	behavior: url('/PIE.htc');
	background: #D1D4D3;
	float: left;
	width: 191px;
	margin: 0 5px;
	padding: 10px 10px 5px 10px;
	position: relative;
	z-index: 1;
	-webkit-border-radius: 5px 5px 0 0;
	-moz-border-radius: 5px 5px 0 0;
	border-radius: 5px 5px 0 0;
	-webkit-box-shadow: 0px 1px 2px 1px #CCC;
	-moz-box-shadow: 0px 1px 2px 1px #CCC;
	box-shadow: 0px 1px 2px 1px #CCC;
	border-top: 1px solid #BCC5C1;
	border-right: 1px solid #BCC5C1;
	border-left: 1px solid #BCC5C1;
}

div#content div.moms-tabs-nav ul.tabs li.active {
	behavior: url('/PIE.htc');
	top: -5px;
	padding: 12px 10px 8px 10px;
	background: #FFF;
	z-index: 15;
	border-top: 1px solid #BCC5C1;
	border-right: 1px solid #BCC5C1;
	border-left: 1px solid #BCC5C1;
	-webkit-box-shadow: 0 -1px 1px -1px #CCC;
	-moz-box-shadow: 0 -1px 1px -1px  #CCC;
	box-shadow: 0 -1px 1px -1px  #CCC;
}

div#content div.moms-tabs-nav ul li a {
	display: block;
	font-size: 12px;
}

div#content div.moms-tabs-nav ul li.active a {
	font-weight: bold;
	color: #9C3022;
}

div#content div.feature.moms-tabs div.pane div.key p {
	font-size: 14px;
	font-weight: bold;
	margin: 40px 0 10px 0;
}

div#content div.feature.moms-tabs div.pane div.key ul li {
    font-weight: bold;
    list-style-type: none;
    padding: 0 0 10px 20px;
	font-size: 11px;
}

div#content div.feature.moms-tabs div.pane div.key ul li.square {
    background: url("assets/images/key-square.jpg") no-repeat scroll 0 2px transparent;
    color: #DC291E;
}

div#content div.feature.moms-tabs div.pane div.key ul li.circle {
    background: url("assets/images/key-circle.png") no-repeat scroll 0 2px transparent;
    color: #1E27AC;
}

div#blog-tools div.search input[type="text"] {
	color: #ccc;
}
div#blog-tools div.search input[type="text"].active {
	color: #000;
}
div#blog-tools div.search input[type="submit"]:hover {
	text-decoration: underline;
	cursor: pointer;
}
div#content div#insight-details.feature div#shopper-profile {
	background: #D9D9D9;
	float: left;
	padding: 10px;
}

div#content div#insight-details.feature div#shopper-profile p {
	margin: 0;
}

div#content div#insight-details.feature div#shopper-profile table {
	width: 255px;
	margin: 0 0 10px 0;
}

div#content div#insight-details.feature div#shopper-profile table td {
	padding: 0;
}

div#content div#insight-details.feature div#shopper-profile div.key {
	position: inline;
	top: 0;
}

div#content div#insight-details.feature div#shopper-profile ul {
	margin: 0;
}

div#content div#insight-details.feature div#shopper-profile ul li {
	padding: 0 0 2px 20px;
}

div#content div#insight-details.feature div.spotlight-ribbon {
	width: 215px;
	height: 125px;
	position: absolute;
	top: -15px;
	right: -15px;
	z-index: 1;
}

div.single-post ul.page-list li ul.children {
	margin: 0;
}

img#wpstats{visibility:hidden; height:0px;}

