function buttonToggle()
{
	btn_title=document.getElementById("case_one_button").value;
	if (btn_title=="Less -"){
		$("#case_one_expanded").slideUp(500);
		document.getElementById("case_one_button").value="More +";
		}
	else{
		$("#case_one_expanded").slideDown(500);
		document.getElementById("case_one_button").value="Less -";
	}
}

function buttonToggle2()
{
	btn_title=document.getElementById("case_two_button").value;
