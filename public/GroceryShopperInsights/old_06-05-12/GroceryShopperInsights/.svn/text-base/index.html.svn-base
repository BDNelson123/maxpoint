<script type="text/javascript">
		
		
		/*Global Variable
		nc_first_name: first name of user
		nc_last_name: last name of user
		nc_email_address: email address of user
		nc_company_name: company name of user
		
		all_blank_fields: flag to see if all fields are blank at the same time
		valid_first_name: flag for the first name field
		valid_last_name: flag for the last name field
		valid_company_name: flag for the company name field
		valid_email: flag for the email field
		*/
		
		nc_first_name = ""
		nc_last_name = ""
		nc_company_name = ""
		nc_email_address = ""
		
		all_blank_fields="true"
		valid_first_name="true";
		valid_last_name="true";
		valid_company_name="true";
		valid_email="true";
		
		
		//---Dispatch Validator() when submit button is clicked---//
		$('#submit').click(function () {
			nc_first_name = $("#first_name").val();
			nc_last_name = $("#last_name").val();
			nc_company_name = $("#company").val();
			nc_email_address = $("#email").val();
			nc_phone = $("#first_name").val();

			validator();
				
			if ((valid_first_name == "false") || (valid_last_name == "false") || (valid_email == "false") || (valid_company_name == "false")) {
				return false;
			}
			else {
				return true;
			}	
        });
		
		
		//---Dispatch Validator() when download button is clicked---//
		$('#download').click(function () {
			nc_first_name = $("#first_name").val();
			nc_last_name = $("#last_name").val();
			nc_company_name = $("#company").val();
			nc_email_address = $("#email").val();
			nc_phone = $("#first_name").val();

			//---If circular download button is pressed and all fields are blank, all fields' background will change to yellow---//
			if ((nc_first_name == "") && (nc_last_name == "") && (nc_email_address == "") && (nc_company_name == "")) {
				document.getElementById('first_name').style.backgroundColor='#FFC';
				document.getElementById('last_name').style.backgroundColor='#FFC';
				document.getElementById('email').style.backgroundColor='#FFC';
				document.getElementById('company').style.backgroundColor='#FFC';
				all_blank_fields ="true";
			}
			else
			{
				all_blank_fields ="false";
				}
			
			//---all fields are blank---//
			if (all_blank_fields == "true"){
				return false;
			}
			else {
				
			validator();
				
			if ((valid_first_name == "false") || (valid_last_name == "false") || (valid_email == "false") || (valid_company_name == "false")) {
				return false;
			}
			else {
				return true;
			}	
				}		
        });
	
		//---Function that validates fields
		function validator() {
			document.getElementById('first_name').style.backgroundColor='#ffffff';
			document.getElementById('last_name').style.backgroundColor='#ffffff';
			document.getElementById('email').style.backgroundColor='#ffffff';
			document.getElementById('company').style.backgroundColor='#ffffff';
			if (nc_first_name == "") {
				$(".hideError_first_name").addClass("showError_first_name");
				valid_first_name="false";
			} 
			else {
				valid_first_name="true";
			}
			
			if (nc_last_name == "") {
				$(".hideError_last_name").addClass("showError_last_name");
				valid_last_name="false";
			} 
			else {
				valid_last_name="true";
			}
			
			var atpos=nc_email_address.indexOf("@");
			var dotpos=nc_email_address.lastIndexOf(".");
			if (atpos<1 || dotpos<atpos+2 || dotpos+2>=nc_email_address.length){
				$(".hideError_email_address").addClass("showError_email_address");
				valid_email="false";
			}
			else {
				valid_email="true";
			}
			
			if (nc_company_name == "") {
				$(".hideError_company_name").addClass("showError_company_name");
				valid_company_name="false";
			} 
			else {
				valid_company_name="true";
			}
		}
		
		//---Remove error message when the field is focused---//
		$('#first_name').focus(function () {
            $(".hideError_first_name").removeClass("showError_first_name");
        });
		$('#last_name').focus(function () {
            $(".hideError_last_name").removeClass("showError_last_name");
        });
		$('#email').focus(function () {
            $(".hideError_email_address").removeClass("showError_email_address");
        });
		$('#company').focus(function () {
            $(".hideError_company_name").removeClass("showError_company_name");
        });
	
    </script> 

